    "apiVersion": "v1",
    "kind": "Service",
    "metadata": {
        "name": "loadbalancer",
        "labels": {
            "app": "nginx",
            "tier": "frontend"
        }
    },
    "spec": {
        "ports": [
            {
                "name": "http",
                "port": 80
