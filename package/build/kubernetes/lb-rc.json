{
    "apiVersion": "v1",
    "kind": "ReplicationController",
    "metadata": {
        "name": "lb-b0aecd6",
        "labels": {
            "app": "nginx",
            "tier": "frontend"
        }
    },
    "spec": {
        "replicas": 2,
        "selector": {
            "app": "nginx",
            "tier": "frontend"
        },
        "template": {
            "metadata": {
                "labels": {
                    "name": "lb",
                    "app": "nginx",
                    "tier": "frontend"
                }
            },
            "spec": {
                "containers": [
                    {
                        "name": "nginx",
                        "image": "gcr.io/m2onk8s/nginx:b0aecd6",
                        "ports": [
                            {
                                "containerPort": 80
                            },
                            {
                                "containerPort": 443
                            }
                        ]
                    }
                ]
            }
        }
    }
}
